{
  "name": "bee",
  "version": "1.0.0",
  "description": "boat bee",
  "author": "",
  "devDependencies": {
    "better-npm-run": "0.1.0",
    "create-validator-ts": "4.0.2",
    "dynamodb-admin": "4.4.0",
    "prettier": "2.7.1",
    "serverless-dynamodb-local": "0.2.40",
    "typescript": "4.8.3",
    "vitest": "0.23.2"
  },
  "scripts": {
    "start": "better-npm-run start",
    "pre-start": "npm install",
    "build": "tsc",
    "fmt": "prettier . --write .",
    "fmt:check": "prettier . --check",
    "test": "vitest",
    "gen": "rm -rf src/model/**/*.validator.ts && npx create-validator-ts src/model/*"
  },
  "betterScripts": {
    "start": {
      "command": "dynamodb-admin",
      "env": {
        "DYNAMO_ENDPOINT": "http://localhost:8000",
        "AWS_REGION": "us-east-1",
        "AWS_ACCESS_KEY_ID": "local",
        "AWS_SECRET_ACCESS_KEY": "local"
      }
    }
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.172.0",
    "@aws-sdk/client-secrets-manager": "3.171.0",
    "@slack/bolt": "3.12.1",
    "ajv": "8.11.0"
  }
}
